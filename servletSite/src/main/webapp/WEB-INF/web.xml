<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" version="6.0">
  <display-name>servletSite</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>HelloServlett</display-name>
    <servlet-name>HelloServlett</servlet-name>
    <servlet-class>com.itwill.servlet.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServlett</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>GugudanServlet</display-name>
    <servlet-name>GugudanServlet</servlet-name>
    <servlet-class>com.itwill.servlet.GugudanServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GugudanServlet</servlet-name>
    <url-pattern>/gugudan</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>HelloTextServlet</display-name>
    <servlet-name>HelloTextServlet</servlet-name>
    <servlet-class>com.itwill.servlet.HelloTextServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloTextServlet</servlet-name>
    <url-pattern>/hellotext</url-pattern>
  </servlet-mapping>
  <servlet>
  <description></description>
    <servlet-name>ServletThreadServlet</servlet-name>
    <servlet-class>com.itwill.servlet.ServletThreadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletThreadServlet</servlet-name>
    <url-pattern>/servletThreadServlet</url-pattern>
  </servlet-mapping>
  
  
  
 
</web-app>